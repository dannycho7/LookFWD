{
	"manifest_version": 2,
	"name": "LookFWD",
	"version": "0.1",
	"content_scripts": [
	  {
	    "matches": ["*://*.youtube.com/"],
	    "js": ["youtube_home.js"]
	  },
	  {
	  	"matches": ["*://*.youtube.com/*"],
	  	"js": ["youtube_video.js"],
      "run_at": "document_start"
	  },
	  {
	  	"matches": ["*://*.facebook.com/*"],
	  	"js": ["facebook.js"]
	  }
	]
}
